<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
	<section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
  </configSections>

  <akka>
	<hocon>
	  <![CDATA[
          akka {
            # here we are configuring log levels
            log-config-on-start = off
            stdout-loglevel = INFO
            loglevel = ERROR
            # this config section will be referenced as akka.actor
            actor {
              provider = "Akka.Remote.RemoteActorRefProvider, Akka.Remote"
              debug {
                  receive = on
                  autoreceive = on
                  lifecycle = on
                  event-stream = on
                  unhandled = on
              }
            }
            # here we're configuring the Akka.Remote module
			  remote {
				helios.tcp {
					transport-class = "Akka.Remote.Transport.Helios.HeliosTcpTransport, Akka.Remote"
					#applied-adapters = []
					transport-protocol = tcp
					port = 0
					hostname = 192.168.1.12
				}
			  log-remote-lifecycle-events = INFO
			}
		  }
		  master {
			  url="akka.tcp://TestSystem@192.168.1.12:8091/user/manager"
		  }
      ]]>
	</hocon>
  </akka>
  <runtime>
	<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
	  <dependentAssembly>
		<assemblyIdentity name="Google.ProtocolBuffers" publicKeyToken="55f7125234beb589" culture="neutral" />
		<bindingRedirect oldVersion="0.0.0.0-2.4.1.555" newVersion="2.4.1.555" />
	  </dependentAssembly>
	</assemblyBinding>
  </runtime>
</configuration>